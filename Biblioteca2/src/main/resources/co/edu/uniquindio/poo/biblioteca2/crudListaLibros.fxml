<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="419.0" prefWidth="616.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.biblioteca2.viewController.ListaLibrosViewController">
   <children>
      <TableView fx:id="tbvTablaListaLibros" layoutX="74.0" layoutY="107.0" prefHeight="244.0" prefWidth="469.0">
