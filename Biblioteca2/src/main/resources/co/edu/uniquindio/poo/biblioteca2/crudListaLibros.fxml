        </columns>
      </TableView>
      <Button fx:id="btnVerInfo" layoutX="258.0" layoutY="371.0" mnemonicParsing="false" onAction="#onVerInfo" text="Ver información" />
      <Label layoutX="163.0" layoutY="46.0" text="Lista de libros registrados">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnRegresar" layoutX="30.0" layoutY="34.0" mnemonicParsing="false" onAction="#onRegresar" text="Regresar" />
